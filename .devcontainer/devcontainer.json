{
  "name": "fusion_etl",
  "dockerComposeFile": "compose.yaml",
  "service": "dev",
  "workspaceFolder": "/opt/dagster/app",
  "shutdownAction": "stopCompose",
  "customizations": {
    "vscode": {
      "settings": {
        "python.defaultInterpreterPath": "/opt/dagster/.venv/bin/python",
        "sqlfluff.executablePath": "/opt/dagster/.venv/bin/sqlfluff",
        "sqlfluff.dialect": "tsql"
      },
      "extensions": [
        "visualstudioexptteam.vscodeintellicode",
        "ms-python.python",
        "charliermarsh.ruff",
        "ms-azuretools.vscode-docker",
        "esbenp.prettier-vscode",
        "github.vscode-github-actions",
        "dorzey.vscode-sqlfluff"
      ]
    }
  }
}
